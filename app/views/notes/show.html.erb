<!-- CSSの読み込み -->
<link type="text/css" rel="stylesheet" href="css/lightGallery.css" />



<%= render 'home/header/dashboardhead' %>

<div class="genre">
 <div class="bootstrap1">

 </div>
</div>

<div class="main-container">

   <% @note_revisions.each do |note_revision| %>
     <% @note.provider.store_infos.each do |s| %>

<div class="provider_wrapper">
  <div class="provider-introduction">
    <h1>  <%= note_revision.title %></h1>

 <% @note_images.each do |note_images| %>
 
    <% if note_images.image1.present? %>
    <%= image_tag note_images.image1_url ,class: "face_pic"%>
    <% else %>
    <%= image_tag 'Seekle_icon.png', :class => 'face_pic'%>
    <% end %>
  

    <div class="star-rating" data-score= <%= @avg_review %> ></div>
    <em><%= "#{@reviews.length} 件の口コミ" %></em>

    <table class="sample_03">
     <tbody>
      <tr>
       <th>提供者：</th>
       <td><%= link_to @note.provider.name, profile_note_path ,:class => "link_provider" %></td>
     </tr>
     <tr>
       <th>エリア：</th>
       <td><%=s.prefecture %></td>
     </tr>
     <tr>
       <th>カテゴリー：</th>
       <td><%= @note.category.name %></td>
     </tr>
     <tr>
       <th>料金：</th>
       <td><%= note_revision.price%>円 〜</td>
     </tr>
   </tbody>
 </table>

 <div class="message_reservetion">
  <div class="bootstrap">


   <%= link_to "お問い合わせする",  new_note_conversation_path(@note),:class =>'btn btn-default' %>

 </div>

 <small>
   <%= link_to "このサービスを依頼", new_note_reservation_path(@note), :class => 'shadow_btn' %>
 </small>
</div>
</div>

<div class="service_explain">
  <h1><%= note_revision.catchcopy %></h1>
</div>

<div class="service_explain">
 <h2>このサービスについて</h2>
 <ul>

  <p><%= simple_format(note_revision.content) %></p>

</ul>
</div>


<div class="service_explain">
 <h2>セールスポイント</h2>
 <ul>

  <p><%= simple_format(note_revision.salespoint) %></p>

</ul>
</div>

<div class="service_explain">
 <h2>変更とキャンセルについて</h2>
 <ul>

  <p><%= simple_format(note_revision.cancelrule) %></p>

</ul>
</div>


<div class="rulesfromprovider">
 <h2>お客様へのお願い</h2>
 <ul>
   <p> <%= simple_format(note_revision.rule) %></p>
</div>


<div class="service_explain">
  <div class="section section--head">

      <div class="row row--heading">
      <h2>画像ギャラリー</h2>
      <p>サービスのイメージ画像</p>
      </div>


  <div class="example gc3">
   
    <div class="html-code grid-of-images">


      <div class="popup-gallery">


 <% @note_images.each do |note_images| %>

      <%= link_to image_path note_images.image1 ,"title"=>"愛うdance" do %>
      <%= image_tag note_images.image1, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image2, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image2, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image3, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image3, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image4, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image4, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image5, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image5, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image6, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image6, width:"75" ,height:"75"%>
      <% end %>

     <%= link_to image_path note_images.image7, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image7, width:"75" ,height:"75"%>
      <% end %>

       <%= link_to image_path note_images.image8, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image8, width:"75" ,height:"75"%>
      <% end %>

      <%= link_to image_path note_images.image9, "data-size" => "1600x1068", "data-med-size" => "1024x683" , "title"=>"dance" do %>
      <%= image_tag note_images.image9, width:"75" ,height:"75"%>
      <% end %>


    <% end %>

       


      </div>
    </div>
    <script type="text/javascript">
      $(document).ready(function() {
        $('.popup-gallery').magnificPopup({
          delegate: 'a',
          type: 'image',
          tLoading: 'Loading image #%curr%...',
          mainClass: 'mfp-img-mobile',
          gallery: {
            enabled: true,
            navigateByImgClick: true,
            preload: [0,1] // Will preload 0 - before current, and 1 after the current image
          },
          image: {
            tError: 'イメージがありません',
             titleSrc: function(item) {
              return item.el.attr('title') ;
            }
          }
        });
      });
    </script>
  </div>



<div class="reviews_providers">
 <h2>レビュー (<%= "#{@reviews.length}" %>)</h2>
 <ul>
   <% if @reviews.blank? %>
   <p>レビューはまだありません</p>
   <% else %>
   <div class="review-wrapper">
    <% @reviews.each do |review| %>
    <div class="reviews">
      <%= review.user.name %>さん
      <div class="star-rating" data-score= <%= review.rating %> ></div>
      <p><%= review.comment %></p>
    </div>
    <% end %>
    <% end %>
    <% end %>
      <% end %>
       <% end %>
  </div>
</ul>
</div>


</div>

</div>
</div>


<script type="text/javascript">
    $(document).ready(function() {
        $("#lightgallery").lightGallery(); 
    });
</script>




<%= render 'providers/header' %>


<div class="main-container">
<div class="providers_signup">

<div class="provider_login">
<h2>サービス管理画面へログイン</h2>
<p><%= link_to "掲載ご希望フォーム" ,"https://goo.gl/forms/kReZeeocLxkidVkE3"%>ご記入後アカウント登録リンクを送らさせていただきます。</p>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


 <div class="field2">
    <h3>メールアドレス</h3>
     
      <%= f.email_field :email, autofocus: true %>
     
    </div>

  <div class="field2">
    <h3>パスワード</h3>

    <%= f.password_field :password, autocomplete: "off" %>
  </div>



  <div class="actions">
    <%= f.submit "ログインする" ,:class => 'provider_join_btn' %>
  </div>


<% end %>

<%= render "providers/shared/links" %>

</div>
</div>
</div>




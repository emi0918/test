<%= render 'home/header/dashboardhead' %>
<div class="main-container">
<div class="bootstrap">

 


  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 <div class="content_in_user">
  <%- flash.each do |name, msg| -%>
    <%= content_tag :div, msg, :id => "error_explanation" if msg.is_a?(String) %>
  <%- end -%>  



    <div class="field">
      <%= f.label :メールアドレス %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>

  
    <div class="actions">
      <%= f.submit "ログイン", class: "log_button", id: "login" %>
      <hr>
    </div>

  <% end %>



  <%= render "devise/shared/links" %>
<%= link_to "パスワードを忘れた", new_password_path(resource_name)%>



<%= link_to "店舗ログイン", provider_session_path ,class:"store_login_btn" %>

</div>
</div>

</div>


